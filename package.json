  "bugs": {
    "url": "https://github.com/cortinabryan/Nature-CSS-SASS---PRACTICE/issues"
  },
  "homepage": "https://github.com/cortinabryan/Nature-CSS-SASS---PRACTICE#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "postcss-cli": "^8.2.0"
  },
  "dependencies": {
    "live-server": "^1.2.1"
  }
}
